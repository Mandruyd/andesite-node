andesite {
  extra-plugins = []
  password = null
  debug-password = null
  log-level = INFO
  auto-ytsearch = true
  audio-handler = magma

  node {
    region = unknown
    id = unknown
  }

  transport {
    http {
      rest = true
      ws = true

      port = 5000
    }
    singyeong {
      enabled = false
      dsn = "singyeong://andesite@localhost:9876"
    }
  }

  prometheus {
    enabled = false
    path = /metrics
  }

  sentry {
    enabled = false
    dsn = null
    tags = []
    log-level = WARN
  }

  lavaplayer {
    frame-buffer-duration = 5000
    non-allocating = true

    youtube {
      max-playlist-page-count = 6
      mix-loader-max-pool-size = 10
    }

    http {
      default = {
        user-agent = my bot,
        cookies = [{
          name = some name,
          value = MyCoolCookie,
          domain = github.com,
          path = /natanbc/andesite,
          secure = true,
          comment = this is my cookie's comment,
          version = 1,
          attributes = {
            some_attribute = some_value
          }
        }]
      },
      youtube = {
        proxy = "http://myproxy.com:1234"
      }
    }
  }

  source {
    bandcamp = true,
    beam = true,
    http = false,
    local = false,
    soundcloud = true,
    twitch = true,
    vimeo = true,
    youtube = true
  }

  lavalink {
    ws-path = /
  }

  magma {
    send-system {
      type = nas

      # Unused if type != nas
      nas-buffer = 400
    }
  }
}
